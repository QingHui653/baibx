@charset "utf-8";
/* CSS Document */

*{ margin: 0; padding: 0;}
ul, ol {list-style: none outside none;}
h1,h2,h3{ font-weight: normal;}
.fl{ float:left; display:inline;}
.fr{ float:right; display:inline;}
img{border:none;}
em,b{ font-style: normal;}
a{ cursor: pointer;}
tbody, thead, tr, th, td {
	border: 0;
	font-size: 100%;
	font-style: inherit;
	font-weight: inherit;
	margin: 0;
	outline: 0;
	padding: 0;
	border-collapse: collapse;
	vertical-align: baseline;
}


#top{ width:100%; height:88px; background:url(../images/top_03.jpg) repeat-x;}
.top_an{ width:83px; height:88px; float:left;}
.top_tit{ width:179px; height:88px; float:left; padding-left:60px;}

#main{ width:100%; text-align:center; padding-top:20px; height:100%; background:#000;}

#img{ width:100%; text-align:center;}


body{height:100%; overflow:hidden; margin:0px; padding:0px;}
.box {height:100%; background:#ff0000; position:absolute; width:100%;} 